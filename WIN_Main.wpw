     name : Click
     procedure_id : 1649154020189057962
     type_code : 14
     code : |1+
      // Syntax:
      //Click (<sVal>)
      //
      // Parameters:
      //	sVal: number of the button (in string format)
      // Return value:
      // 	None
      
      PROCEDURE Click(sVal)
      
      // Is this square free?
      IF {"IMAGE"+sVal,indControl}<>"" THEN RETURN
      
      // The player plays
      IF PlayerPlays(sVal) = True THEN
      	
      	// The computer plays
      	ComputerPlay()
      END
     type : 458752
   -
     name : Plays
     procedure_id : 1649154020189123498
     type_code : 14
     code : |1-
      // Syntax:
      //Play (<nSquare> [, <sImage> is UNICODE string])
      //
      // Parameters:
      //	nSquare: Number of the square to play
      //	sImage (UNICODE string): Image that must be assigned to the square
      // Return value:
      // 	None
      
      PROCEDURE Plays(nCell,sImage is string=gsPCImg)
      
      // Place the "mark" in the square
      {"IMAGE"+nCell,indControl}=sImage
     type : 458752
   -
     name : PlayerPlays
     procedure_id : 1649154020189189034
     type_code : 14
     code : |1+
      // Syntax:
      //[ <Result> = ] PlayerPlays (<sVal>)
      //
      // Parameters:
      //	sVal: number of the button (in string format)
      // Return value:
      // 	boolean:  // 	None
      
      PROCEDURE PlayerPlays(sVal)
      
      // Check whether the square is not empty
      IF {"IMAGE"+sVal,indControl}<>"" THEN RESULT False
      
      // Play this "mark"
      Plays(Val(sVal),gsPlayerImg)
      
      // Fill a table in memory with the game status:
      FillGameTable()
      
      // stop if the player won the game:
      IF bWon(pManageSquare.garrCell)=True THEN
      	OpenMobileWindow(WIN_Result,cnVictory)
      	RESULT False
      ELSE
      	IF bFullTable()=True THEN
      		OpenMobileWindow(WIN_Result,cnDraw)
      		RESULT False
      	END	
      END
      
      
      // The game continues by default
      RESULT True
     type : 458752
   -
     name : nRecurs
     procedure_id : 1649154020189254570
     type_code : 14
     code : |1+
      // Description of input/output parameters of 'nRecurs':
      //
      // Syntax:
      //[ <Result> = ] nRecurs (<ttabSquare>, <nLevel>, <nSquareSubscript>, <nLevelWon>)
      //
      // Parameters:
      //	tarrSquare: 		the game board (by address, so don't modify it)
      //	nLevel: 		current level in exploitation
      //	nSquareSubscript: 	will contain the number of the square to play.
      //	nLevelWon:	level that corresponds to the winning move
      // Return value:
      // 	Undefined type: // 	
      
      PROCEDURE nRecurs(tarrSquare, nLevel, nSquareSubscript,nLevelWon)
      
      i, j					are int
      Row, Col				are int
      RowPlayer, ColPlayer	are int
      n						is int
      nRes					is int
      nCell					is int
      arrLocal				is array of 3 by 3 int
      
      // Don't explore the levels until the end to leave a chance
      IF nLevel>4 THEN RESULT 0
      
      // copy of the array (do not pass by value for the array in WLanguage)
      ArrayCopy(tarrSquare,arrLocal)
      
      // Player's turn:
      FOR i=1 _TO_ pManageSquare.gnMaxSquare
      	Row=nCalcRow(i)
      	Col=nCalcCol(i)
      	
      	// if the square is free (otherwise, go to the next one):
      	IF arrLocal[Col][Row]=0 THEN
      		// the computer plays, let's see...
      		arrLocal[Col][Row]=2
      		// does the computer win with this cell:
      		IF bWon(arrLocal) THEN
      			// yes => fill the result variable.
      			IF nLevel<nLevelWon THEN
      				nSquareSubscript=i
      				nLevelWon=nLevel+1
      				RESULT nLevel+1
      			END
      		END
      		
      		nRes=LIMIT_MAX_INT_4
      		
      		// Recursion:
      		FOR j=1 _TO_ pManageSquare.gnMaxSquare
      			RowPlayer=nCalcRow(j)
      			ColPlayer=nCalcCol(j)
      			
      			IF arrLocal[ColPlayer][RowPlayer]=0 THEN
      				// the player plays:
      				arrLocal[ColPlayer][RowPlayer]=1
      				
      				IF bWon(arrLocal) THEN
      					nSquareSubscript=j
      					nLevelWon=nLevel+2
      					RESULT -(nLevel+2)
      				END
      				
      				n=nRecurs(arrLocal,nLevel+2,nCell,nLevelWon)
      				IF n<>0 THEN
      					IF n=-(nLevel+2) THEN	RESULT n		// no need to continue, 
      					// the player has won.
      					IF Abs(n)<Abs(nRes) THEN
      						nRes=n
      						nSquareSubscript=nCell
      					END
      				END
      				arrLocal[ColPlayer][RowPlayer]=0
      			END
      		END
      		
      		// restore the square as it was and go to the next one:
      		arrLocal[Col][Row]=0
      	END
      END
      
      IF nRes<>LIMIT_MAX_INT_4 THEN RESULT nRes
      
      RESULT 0
     type : 458752
   -
     name : ComputerPlay
     procedure_id : 1649154020189320106
     type_code : 14
     code : |1+
      // Syntax:
      //ComputerPlay ()
      //
      // Parameters:
      //	None
      
      // Return value:
      PROCEDURE ComputerPlay()
      
      // Local variables
      nSquareToPlay			is int
      
      // Search for the square to play
      n	is int=nRecurs(pManageSquare.garrCell,1,nSquareToPlay,LIMIT_MAX_INT_4)
      
      // Did you find an "interesting" square to play?
      IF nSquareToPlay<>0 THEN
      	// Yes, play this square
      	Plays(nSquareToPlay,gsPCImg)
      ELSE
      	// equality, 
      	// search for the first available space:
      	Plays(nNumEmptySquare,gsPCImg)
      END
      
      // Fill a table in memory with the game status:
      FillGameTable()
      
      // Was the game won
      IF bWon(pManageSquare.garrCell) THEN 
      	OpenMobileWindow(WIN_Result,cnDefeat)
      ELSE
      	IF bFullTable()=True THEN
      		OpenMobileWindow(WIN_Result,cnDraw)
      	END	
      END
     type : 458752
   -
     name : FillGameTable
     internal_properties : CAAAAAgAAADyPvxb05hzit6JChhQbN/PuY6v/cO+r2QywwOzLwv3zcSf1gqKT5zZPNxWmLypB4w74/QUxdmW0IPLNoCn7dbha4+b/rJYbdr+5gTOrjoxNpz/j3LBHfqXl140U5oda+ZYl/7XYk2Za2yRiuEal0I3ygwblo80Iq1ONf2kPAvDSCbtEDOnE4teLaj6+x+og+AbUu6H
     procedure_id : 1649154020189385642
     type_code : 14
     code : |1+
      // Syntax:
      //FillGameTable ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      PROCEDURE FillGameTable()
      
      i			is int
      Row, Col		are int
      
      
      // Fill a table in memory with the game status:
      FOR i=1 _TO_ pManageSquare.gnMaxSquare
      	// Search for the row and column corresponding to the nth square
      	Row=nCalcRow(i)
      	Col=nCalcCol(i)
      	
      	// Assign according to the image found
      	SWITCH {"IMAGE"+i,indControl}
      		CASE gsPlayerImg				// Player 1: "Human" player
      			
      			AssignSquare(Row,Col,1)
      		CASE gsPCImg				// Player 2: "Computer" player
      			
      			AssignSquare(Row,Col,2)
      		OTHER CASE					// Empty square
      			
      			AssignSquare(Row,Col,0)
      			gbNotEnded=True
      	END
      END
      
     type : 458752
   -
     name : ResetGame
     procedure_id : 1649154020189451178
     type_code : 14
     code : |1-
      PROCEDURE ResetGame()
      
      // Local variables
      i	is int
      
      // Initialize the graphic part
      FOR i=1 TO 9
      	{"IMAGE"+i,indControl}=""	
      END
      
      // Initialize the game board
      FillGameTable()
      
      // Draw the first player
      DrawFirstPlayer()
      
      // The computer plays if the "human" player is not the first player
      IF gbPlayerStarts = False THEN
      	ComputerPlay()
      END
     type : 458752
   -
     name : DrawFirstPlayer
     procedure_id : 1649154020189516714
     type_code : 14
     code : |1+
      // Syntax:
      //DrawFirstPlayer ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      PROCEDURE DrawFirstPlayer()
      
      // Initialize the random engine
      InitRandom()
      
      // Draw the first player (1 if the "Human" player starts)
      gbPlayerStarts = (Random()>0.5)
      
      // Display the first player
      IF gbPlayerStarts=True THEN
      	Info(<§$0005§>)	
      ELSE
      	Info(<§$0006§>)	
      END
      
     type : 458752
